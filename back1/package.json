{
  "name": "back1",
  "version": "0.0.1",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist_tool_v1_api/index.js",
      "require": "./dist_tool_v1_api/index.js",
      "types": "./dist_tool_v1_api/index.d.ts"
    },
