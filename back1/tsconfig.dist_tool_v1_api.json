{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist_tool_v1_api",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolvePackageJsonExports": false,
    "noEmit": false,
    "incremental": false,
    "skipLibCheck": true,
    // 路径别名
    "paths": {
      "@src/*": ["./src/*"]
    }
  },
  // "include": ["src/dist_tool_v1_api/**/*", "src/v1/auth/dto/login.ts"],
  "include": ["src/dist_tool_v1_api/**/*",],
  "exclude": ["node_modules", "dist_tool_v1_api"]
}
